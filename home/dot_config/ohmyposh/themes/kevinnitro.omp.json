{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    // "os": "#CDD6F4",
    // "flamingo": "#F2CDCD",
    // "pink": "#F5BDE6",
    // "mauve": "#CBA6F7",
    // "red": "#F38BA8",
    // "marron": "#EBA0AC",
    // "yellow": "#F9E2AF",
    // "green": "#A6E3A1",
    // "teal": "#94E2D5",
    // "sky": "#89DCDB",
    // "sapphire": "#74C7EC",
    "blue": "#89B4FA",
    "lavender": "#B7BDF8",
    "text": "#494D64"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "folder_icon": " \ue5ff ",
            "home_icon": "~",
            "style": "agnoster_short",
            "folder_separator_icon": "/",
            "hide_root_location": true,
            "max_depth": 2
          },
          "style": "powerline",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "background": "p:lavender",
          "foreground": "p:text",
          "style": "powerline",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "powerline_symbol": "\ue0b4",
          "trailing_diamond": "\ue0b4",
          "leading_diamond": "\ue0b6",
          "template": " {{ .HEAD }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
